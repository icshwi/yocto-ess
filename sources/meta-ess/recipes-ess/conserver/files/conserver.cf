# Ansible managed: Do NOT edit this file manually!

config * {
	logfile /var/log/conserver/conserver.log;
}

default full    { rw *; }
default * {
        logfile /var/log/conserver/&.log;
        timestamp "";
        include full;
        master localhost;
}

# Create a noop console so that conserver can start
# with no IOCs defined
console noop {
	master localhost;
	type noop;
	motd "Dummy console, does nothing";
}
